import java.util.*;
import java.io.*;

class Solution {
    public int solution(String[][] clothes) {
        int answer = 1;
        HashMap<String, Integer> map = new HashMap<>();
        for(String[] c : clothes){
            map.put(c[1], map.getOrDefault(c[1], 0)+1);
        }
        System.out.println(map);
        
        for(String s : map.keySet()) {
            answer *= (map.get(s)+1);
        }
       
        return answer-1;
    }
}
