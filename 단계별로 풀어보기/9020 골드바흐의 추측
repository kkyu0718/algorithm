count = int(input())


def is_prime(num):
    for i in range(2, num):
        if num % i == 0:
            return False
    return True

for i in range(count):
    num = int(input())
    x , y = int(num /2) , int(num / 2)


    while(True):
        if (is_prime(x) == True ) & (is_prime(y) == True ) :
            print(x, y)
            break
        x = x-1
        y = y+1
